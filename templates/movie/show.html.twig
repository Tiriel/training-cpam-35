{% extends 'base.html.twig' %}

{% block title %}{{ movie.title }}{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>{{ movie.title }}</h1>

    <ul>
        <li>Id : {{ movie.id }}</li>
        <li>Released : {{ movie.released|date('d M Y') }}</li>
        <li>Country : {{ movie.country }}</li>
        <li>Genre : {{ movie.genre|join(', ') }}</li>
    </ul>
</div>
{% endblock %}
